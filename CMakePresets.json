{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build",
            "displayName": "Build for nRF9160 DK NRF9160 Non Secure",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "nrf9160dk_nrf9160_ns",
                "BOARD_ROOT": "${sourceDir}/",
                "DTC_OVERLAY_FILE": "${sourceDir}/boards\\nrf9160dk_nrf9160_ns.overlay",
                "CONF_FILE": "${sourceDir}/prj.conf;${sourceDir}/boards\\nrf9160dk_nrf9160_ns.conf",
                "OVERLAY_CONFIG": "${sourceDir}/overlay-avsystem.conf;${sourceDir}/overlay-lwm2m-1.1.conf;${sourceDir}/overlay-nbiot.conf;${sourceDir}/overlay-assist-cell.conf;${sourceDir}/overlay-adv-firmware.conf;${sourceDir}/boards\\nrf9160dk_nrf9160_ns.conf"
            }
        }
    ]
}